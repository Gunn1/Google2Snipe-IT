################################################################################
# Google2Snipe-IT Environment Configuration Example
#
# Copy this file to .env and fill in your actual values.
# DO NOT commit .env to version control - it contains secrets!
################################################################################

# ==================== SNIPE-IT Configuration ====================
# Your Snipe-IT API Token for authentication
API_TOKEN=your_snipeit_api_key_here

# Your Snipe-IT instance API endpoint
# Format: https://your-instance.com/api/v1
ENDPOINT_URL=https://your-snipeit-instance/api/v1

# Default Model ID to use when a new model can't be created
# Find this by checking an existing model in your Snipe-IT instance
SNIPE_IT_DEFAULT_MODEL_ID=87

# Fieldset ID to assign to newly created models
# This fieldset should contain your custom fields
SNIPE_IT_FIELDSET_ID=9

# Custom field IDs in your Snipe-IT instance
# Find these in Settings â†’ Custom Fields
SNIPE_IT_FIELD_MAC_ADDRESS=_snipeit_mac_address_1
SNIPE_IT_FIELD_SYNC_DATE=_snipeit_sync_date_9
SNIPE_IT_FIELD_IP_ADDRESS=_snipeit_ip_address_3
SNIPE_IT_FIELD_USER=_snipeit_user_10


# ==================== Google Workspace Configuration ====================
# Email of the admin user that the service account will impersonate
# This user must have appropriate permissions to access ChromeOS devices
DELEGATED_ADMIN=admin@your-domain.com

# Path to your Google service account JSON key file
# Download this from Google Cloud Console
GOOGLE_SERVICE_ACCOUNT_FILE=service_account.json

# Number of devices to fetch per Google API page (max 300)
GOOGLE_CHROMEOS_PAGE_SIZE=300

# Projection level for ChromeOS device data (BASIC or FULL)
GOOGLE_CHROMEOS_PROJECTION=FULL


# ==================== Google Gemini Configuration ====================
# Your Google Gemini API key for AI-powered model categorization
# Get this from Google AI Studio (https://aistudio.google.com/app/apikeys)
Gemini_APIKEY=your_gemini_api_key_here

# Model to use for categorization (optional)
GEMINI_MODEL=gemini-1.5-flash

# Comma-separated list of valid categories for device classification
GEMINI_CATEGORIES=IMac,Tablets,Mobile Devices,Servers,Networking Equipment,Printers & Scanners,Desktop,Chromebook


# ==================== Application Configuration ====================
# Set to 'production' for scheduled execution, 'development' for testing
ENVIRONMENT=development

# Enable debug logging (verbose output)
DEBUG=false

# Enable dry-run mode (no actual changes to Snipe-IT)
DRY_RUN=false


# ==================== Retry Configuration ====================
# Maximum number of retries for failed API requests
MAX_RETRIES=4

# Delay between retries in seconds
RETRY_DELAY_SECONDS=20

# Backoff factor for exponential backoff (1.0 = no backoff)
RETRY_BACKOFF_FACTOR=1.0


# ==================== Logging Configuration ====================
# File to write error logs to
LOG_FILE=snipeit_errors.log

# Minimum log level: DEBUG, INFO, WARNING, ERROR, CRITICAL
LOG_LEVEL=WARNING

# Log message format
LOG_FORMAT=%(asctime)s - %(name)s - %(levelname)s - %(message)s
